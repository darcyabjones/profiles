#%Module1.0#####################################################################
##
## $name modulefile
##

set ver [lrange [split [ module-info name ] / ] 1 1 ]
set name [lrange [split [ module-info name ] / ] 0 0 ]

proc ModulesHelp { } {
  puts stderr "\tThis module sets the envinronment for $name"
}

module-whatis   "Set environment variables to use $name"

if { ![ is-loaded java ] } {
  module load java
}

if { ![ is-loaded perl ] } {
  module load perl
}

if { ![ is-loaded gcc/4.1.2 ] } {
  module load gcc/4.1.2
}

#prepend-path LD_LIBRARY_PATH /usr/local/$name/$ver/lib
prepend-path PATH /usr/local/$name/$ver/I-TASSERmod
prepend-path PATH /usr/local/$name/$ver/bin

setenv libdir /usr/local/$name/$ver/

system /usr/local/vlsci/sbin/test_group_member itasser

