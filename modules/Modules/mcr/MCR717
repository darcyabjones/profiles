#%Module1.0#####################################################################
##
## $name modulefile
##

set ver [lrange [split [ module-info name ] / ] 1 1 ]
set name mcr

proc ModulesHelp { } {
  puts stderr "\tThis module sets the envinronment for $name v$ver"
}

module-whatis   "Set environment variables to use $name version $ver"

prepend-path PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/bin
prepend-path LD_LIBRARY_PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/runtime/glnxa64
prepend-path LD_LIBRARY_PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/bin/glnxa64
prepend-path LD_LIBRARY_PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/sys/os/glnxa64
prepend-path LD_LIBRARY_PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/sys/java/jre/glnxa64/jre/lib/amd64/native_threads
prepend-path LD_LIBRARY_PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/sys/java/jre/glnxa64/jre/lib/amd64/server
prepend-path LD_LIBRARY_PATH /usr/local/$name/MATLAB_Compiler_Runtime/v717/sys/java/jre/glnxa64/jre/lib/amd64

setenv XAPPLRESDIR /usr/local/$name/MATLAB_Compiler_Runtime/v717/X11/app-defaults
setenv MCRROOT /usr/local/$name/MATLAB_Compiler_Runtime/v717
