#%Module1.0#####################################################################
##
## $name modulefile
##

set ver [lrange [split [ module-info name ] / ] 1 1 ]
set name [lrange [split [ module-info name ] / ] 0 0 ]
set subname [lrange [split $name - ] 0 0 ]
set compiler [lrange [split $name - ] 1 1 ]

proc ModulesHelp { } {
  puts stderr "\tThis module sets the envinronment for $name v$ver"
}

module-whatis   "Set environment variables to use $name version $ver"

#if { [ is-loaded intel ] } {
#  module unload intel
#}
#module unload intel
#module unload openmpi

setenv  OPENCMISS_ROOT        /usr/local/$subname/$ver-$compiler/OpenCMISS
setenv  OPENCMISSEXTRAS_ROOT       /usr/local/$subname/$ver-$compiler/OpenCMISSExtras

prepend-path PATH /usr/local/$subname/$ver-$compiler/OpenCMISSExtras/cm/external/x86_64-linux-debug/openmpi/gnu_4.4/bin
prepend-path PATH /usr/local/$subname/$ver-$compiler/OpenCMISSExtras/mpi/x86_64-linux-debug/openmpi/gnu_4.4/bin
prepend-path PATH /usr/local/$subname/$ver-$compiler/OpenCMISSExtras/cellml/x86_64-linux-debug/openmpi/gnu_4.4/bin
prepend-path MANPATH /usr/local/$subname/$ver-$compiler/OpenCMISSExtras/mpi/x86_64-linux-debug/openmpi/gnu_4.4/share/man

setenv 	PYTHONPATH /usr/local/$subname/$ver-$compiler/OpenCMISS/cm/bindings/python
setenv	PETSC_DIR /usr/local/$subname/$ver-$compiler/OpenCMISSExtras/cm/external/x86_64-linux-debug/openmpi/gnu_4.4

#setenv	OPENCMISSEXAMPLES_ROOT /usr/local/$subname/$ver-$compiler/OpenCMISS/examples
setenv	INTELAPI intel64
setenv 	SYSLIBAPI lib64
setenv	TOTALVIEW_VERSION 8.9.0-2
setenv	OPENCMISS_REDHAT_RELEASE 6.4
setenv	INTEL_MPI_VERSION 4.1.0.024
setenv	BINAPI	bin64
setenv	GNU_MINOR_VERSION 4
setenv	OPENCMISSEXTRAS_RUN_CONFIG debug
setenv	OPENCMISS_COMPILER_PATH gnu_4.4
setenv	GNU_COMPILER_VERSION 4.4
setenv	GNU_MAJOR_VERSION 4
setenv	TOTALVIEW_PATH /usr/toolsworks
setenv	OPENCMISS_GFORTRAN_LIBPATH /usr/lib/gcc/x86_64-redhat-linux/4.4.7
setenv  OPENCMISS_GFORTRAN_EXISTS true
setenv	LIBAPI lib64
setenv	OPENCMISSEXTRAS_ARCHITECTURE x86_64-linux-debug
setenv	archname x86_64-linux
setenv	OPENCMISS_MPI_PATH openmpi
setenv	FLFXLM_VERSION 10.8.0-3
setenv	OPENCMISS_LINUX_DISTRIBUTION redhat
setenv	OPENCMISS_GFORTRAN_LIBGCC /usr/lib/gcc/x86_64-redhat-linux/4.4.7/libgcc.a
setenv	TAUAPI x86_64
setenv	COMPILER gnu


setenv	OPENCMISS_USE_CELLML true
