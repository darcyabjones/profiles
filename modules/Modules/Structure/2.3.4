#%Module1.0#####################################################################
##
## $name modulefile
##

set ver [lrange [split [ module-info name ] / ] 1 1 ]
set name [lrange [split [ module-info name ] / ] 0 0 ]

module-whatis   "Set environment variables to use $name version $ver, defaults to gcc version"

if { ![ is-loaded Structure-gcc ] } {
  module load Structure-gcc
}
